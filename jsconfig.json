{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["es2020", "dom"],
    "noEmit": true,
    "module": "esnext",
    "strict": false,
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
  },
  "exclude": ["node_modules"]
}
